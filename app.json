{
  "expo": {
    "name": "AvtoToza",
    "slug": "avtotoza-mobile",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icons/icon_snow_512_playstore.png",
    "scheme": "avtotozamobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "uz.avtotoza.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSUserTrackingUsageDescription": "Мы используем данные для аналитики и рекламы"
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/icons/icon_snow_512_playstore.png",
        "backgroundImage": "./assets/icons/icon_snow_512_playstore.png",
        "monochromeImage": "./assets/icons/icon_snow_512_playstore.png"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCMg3VXZI6-hBX2tSFZPudvNbO_Bu1nHMY"
        }
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "uz.avtotoza.app"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/icon_snow_512_playstore.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "@react-native-community/datetimepicker",
      "expo-web-browser",
      "expo-localization",
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "94db1a3f-f289-4bf7-a96b-04a3081bb69b"
      }
    }
  }
}
